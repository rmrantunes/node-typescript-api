  "bugs": {
    "url": "https://github.com/rmrantunes/node-typescript-api/issues"
  },
  "homepage": "https://github.com/rmrantunes/node-typescript-api#readme",
  "devDependencies": {
    "@types/config": "0.0.37",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.19",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^14.14.14",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "eslint": "^7.16.0",
    "jest": "^26.6.3",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.5",
    "@types/axios": "^0.14.0",
