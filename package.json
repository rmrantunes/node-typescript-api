{
  "name": "typescript-api",
  "version": "1.0.0",
  "description": "Typescript Nodejs API",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/src/index.js",
    "start:local": "ts-node-dev src/index.ts | pino-pretty -c",
    "test": "npm run test:unit && npm run test:functional",
    "test:functional": "jest --projects ./test --runInBand",
    "test:unit": "jest",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rmrantunes/node-typescript-api.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rmrantunes/node-typescript-api/issues"
  },
  "homepage": "https://github.com/rmrantunes/node-typescript-api#readme",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/config": "0.0.37",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/express-pino-logger": "^4.0.2",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^26.0.19",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/module-alias": "^2.0.0",
    "@types/nock": "^11.1.0",
    "@types/node": "^14.14.14",
    "@types/pino": "^6.3.4",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "eslint": "^7.16.0",
    "jest": "^26.6.3",
    "nock": "^13.0.5",
    "pino-pretty": "^4.3.0",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.5",
    "@types/axios": "^0.14.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "config": "^3.3.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-pino-logger": "^5.0.0",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongoose": "^5.11.8",
    "pino": "^6.9.0"
  }
}
